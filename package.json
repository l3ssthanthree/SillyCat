{
  "name": "silly-cat",
  "displayName": "Silly Cat",
  "description": "Silly Cat shows you cat faces that correlate to the number of errors in your code!",
  "version": "0.0.1",
  "publisher": "l3ssthanthree",
  "homepage": "https://github.com/l3ssthanthree/SillyCat",
  "repository": {
    "type": "git",
    "url": "https://github.com/l3ssthanthree/SillyCat"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Visualization"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "keywords": [
    "silly cat",
    "error count"
  ],
  "__metadata": {
    "publisherDisplayName": "l3ssthanthree"
  },
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [{
        "id": "custom-activitybar",
        "title": "Silly Cat",
        "icon": "assets/logo_bito.svg"
      }]
    },
    "views": {
      "custom-activitybar": [{
        "type": "webview",
        "id": "silly-cat.openview",
        "name": "Silly Cat",
        "contextualTitle": "Silly Cat"
      }]
    },
    "configuration": {
      "title": "Silly Cat",
      "properties": {
        "SillyCat.error.usewarnings": {
          "type": "boolean",
          "default": false,
          "description": "Should take into account warnings"
        }
      }
    },
    "commands": [{
      "command": "silly-cat.openview.focus",
      "category": "Silly Cat",
      "title": "Open Sidebar",
      "icon": "$(clear-all)"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  }
}
